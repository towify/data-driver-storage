{
  "name": "@towify/document-generator",
  "description": "towify document generator",
  "license": "MIT",
  "author": "Towify",
  "version": "0.0.1",
  "main": "./lib/index.js",
  "type": "module",
  "scripts": {
    "tsc": "tsc",
    "publish": "node npm-publish-cli.js",
    "test": "jest",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "jest": {
    "preset": "ts-jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.html": "prettier --write --parser angular",
    "*.component.html": "prettier --write --parser angular",
    "*.{js,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.js": "eslint --cache --fix",
    "*.{js,css,md,json,scss}": "prettier --write"
  },
  "peerDependencies": {},
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@types/jest": "^28.1.6",
    "@types/node": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/eslint-plugin-tslint": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "eslint": "^8.20.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "latest-version": "^6.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.4"
  },
  "keywords": [
    "cli",
    "nodejs",
    "towify",
    "typescript"
  ]
}
